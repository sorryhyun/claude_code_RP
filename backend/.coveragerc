[run]
source = .
omit =
    */tests/*
    */conftest.py
    */venv/*
    */__pycache__/*
    */migrations.py
    */generate_hash.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

[html]
directory = htmlcov
